@model IEnumerable<GestaoPI.Models.ServicoPatente>
    @{ ViewData["Title"] = "Servicos"; }

    <h1>@ViewData["Title"]</h1>

    <div class="d-flex justify-content-end my-3">
        <button type="button" class="btn btn-primary" data-bs-toggle="modal" data-bs-target="#exampleModal">
            Cadastrar
        </button>
    </div>

    <table class="table table-striped table-bordered">
        <thead class="text-center">
            <tr>
                <td>@Html.DisplayNameFor(model => model.CodigoServico)</td>
                <td>@Html.DisplayNameFor(model => model.Data)</td>
                <td>Detalhes</td>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model) {
            <tr>
                <td>@Html.DisplayFor(modelItem => item.CodigoServico)</td>
                <td>@Html.DisplayFor(modelItem => item.Data)</td>
                <td class="text-center">
                    <a asp-action="Details" asp-route-id="@item.CodigoServico">
                        <i class="bi bi-eye"></i>
                    </a>
                </td>
            </tr>
            }
        </tbody>
    </table>
</GestaoPI.Models.ServicoPatente>
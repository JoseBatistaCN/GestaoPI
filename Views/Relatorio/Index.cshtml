@{ViewData["Title"] = "Relatórios";}

<h1>@ViewData["Title"]</h1>

<div class="card">
  <div class="card-body">
    <div class="mb-3">
      <label for="" class="form-label">Número do Processo</label>
      <input type="text" class="form-control" name="" id="" aria-describedby="helpId" placeholder="">
    </div>
    <div class="form-check">
      <input class="form-check-input" type="radio" name="" id="">
      <label class="form-check-label" for="">
        Despachos do Processo
      </label>

    </div>
  </div>
</div>




<div>
  <div align="center">
    <table width="100%">
      <tbody>
        <tr>
          <td width="30%"></td>
          <td align="left">
            <table id="ctl00_ContentPlaceHolder1_radLstRelatorio" style="font-weight: bold; width: 100%">
              <tbody>
                <tr>
                  <td>
                    <span class="radio"><input id="ctl00_ContentPlaceHolder1_radLstRelatorio_0" type="radio"
                        name="ctl00$ContentPlaceHolder1$radLstRelatorio" value="Despachos de um Processo (Histórico)"
                        checked="checked" /><label for="ctl00_ContentPlaceHolder1_radLstRelatorio_0">Despachos de um
                        Processo (Histórico)</label></span>
                  </td>
                </tr>
                <tr>
                  <td>
                    <span class="radio"><input id="ctl00_ContentPlaceHolder1_radLstRelatorio_1" type="radio"
                        name="ctl00$ContentPlaceHolder1$radLstRelatorio"
                        value="Data de Vencimento - Despachos do Processo "
                        onclick="javascript:setTimeout('__doPostBack(\'ctl00$ContentPlaceHolder1$radLstRelatorio$1\',\'\')', 0)" /><label
                        for="ctl00_ContentPlaceHolder1_radLstRelatorio_1">Data de Vencimento - Despachos do Processo
                      </label></span>
                  </td>
                </tr>
                <tr>
                  <td>
                    <span class="radio"><input id="ctl00_ContentPlaceHolder1_radLstRelatorio_2" type="radio"
                        name="ctl00$ContentPlaceHolder1$radLstRelatorio" value="Data de Vencimento do Exame"
                        onclick="javascript:setTimeout('__doPostBack(\'ctl00$ContentPlaceHolder1$radLstRelatorio$2\',\'\')', 0)" /><label
                        for="ctl00_ContentPlaceHolder1_radLstRelatorio_2">Data de Vencimento do Exame</label></span>
                  </td>
                </tr>
                <tr>
                  <td>
                    <span class="radio"><input id="ctl00_ContentPlaceHolder1_radLstRelatorio_3" type="radio"
                        name="ctl00$ContentPlaceHolder1$radLstRelatorio" value="Data de Vencimento das Anuidades"
                        onclick="javascript:setTimeout('__doPostBack(\'ctl00$ContentPlaceHolder1$radLstRelatorio$3\',\'\')', 0)" /><label
                        for="ctl00_ContentPlaceHolder1_radLstRelatorio_3">Data de Vencimento das
                        Anuidades</label></span>
                  </td>
                </tr>
                <tr>
                  <td>
                    <span class="radio"><input id="ctl00_ContentPlaceHolder1_radLstRelatorio_4" type="radio"
                        name="ctl00$ContentPlaceHolder1$radLstRelatorio" value="Processos por Inventor (Situação atual)"
                        onclick="javascript:setTimeout('__doPostBack(\'ctl00$ContentPlaceHolder1$radLstRelatorio$4\',\'\')', 0)" /><label
                        for="ctl00_ContentPlaceHolder1_radLstRelatorio_4">Processos por Inventor (Situação
                        atual)</label></span>
                  </td>
                </tr>
                <tr>
                  <td>
                    <span class="radio"><input id="ctl00_ContentPlaceHolder1_radLstRelatorio_5" type="radio"
                        name="ctl00$ContentPlaceHolder1$radLstRelatorio"
                        value="Situação Atual dos Processos - Por Despacho"
                        onclick="javascript:setTimeout('__doPostBack(\'ctl00$ContentPlaceHolder1$radLstRelatorio$5\',\'\')', 0)" /><label
                        for="ctl00_ContentPlaceHolder1_radLstRelatorio_5">Situação Atual dos Processos - Por
                        Despacho</label></span>
                  </td>
                </tr>
                <tr>
                  <td>
                    <span class="radio"><input id="ctl00_ContentPlaceHolder1_radLstRelatorio_6" type="radio"
                        name="ctl00$ContentPlaceHolder1$radLstRelatorio"
                        value="Situação Atual dos Processos - Sintético (Todos)"
                        onclick="javascript:setTimeout('__doPostBack(\'ctl00$ContentPlaceHolder1$radLstRelatorio$6\',\'\')', 0)" /><label
                        for="ctl00_ContentPlaceHolder1_radLstRelatorio_6">Situação Atual dos Processos - Sintético
                        (Todos)</label></span>
                  </td>
                </tr>
              </tbody>
            </table>
          </td>
        </tr>
      </tbody>
    </table>

    <div align="right">
      <input type="submit" name="ctl00$ContentPlaceHolder1$btnVisualizar" value="Visualizar"
        onclick='javascript:WebForm_DoPostBackWithOptions(new WebForm_PostBackOptions("ctl00$ContentPlaceHolder1$btnVisualizar", "", true, "", "", false, false))'
        id="ctl00_ContentPlaceHolder1_btnVisualizar" class="btn btn-inverse" />
      <input type="submit" name="ctl00$ContentPlaceHolder1$btnVoltar" value="Voltar"
        id="ctl00_ContentPlaceHolder1_btnVoltar" class="btn" />
    </div>

    <div align="center">
      <div id="ctl00_ContentPlaceHolder1_pnlRelatorio2" style="width: 100%">
        <table border="0" cellpadding="1" cellspacing="2" style="width: 100%">
          <tbody>
            <tr>
              <td style="width: 50%; text-align: right">
                Informe o Número do Processo:
              </td>
              <td style="width: 50%; text-align: left">
                <input name="ctl00$ContentPlaceHolder1$txtNumPatente" type="text" maxlength="19"
                  id="ctl00_ContentPlaceHolder1_txtNumPatente" style="width: 165px" />
                <span id="ctl00_ContentPlaceHolder1_valReqPatente" style="display: none">*</span>
                <span id="ctl00_ContentPlaceHolder1_valCusNumPatente" style="display: none">*</span>
              </td>
            </tr>
            <tr>
              <td align="center" colspan="2"></td>
            </tr>
          </tbody>
        </table>
      </div>

      <div class="centerComponente">
        <div id="ctl00_ContentPlaceHolder1_valSumario" class="alert alert-danger" style="display: none"></div>
      </div>
    </div>
  </div>
</div>
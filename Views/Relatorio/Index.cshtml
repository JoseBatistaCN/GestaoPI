@{ViewData["Title"] = "Relatórios";}

<h1>@ViewData["Title"]</h1>

<div class="card">
  <div class="card-body">
    <div class="mb-3">
      <label for="" class="form-label">Número do Processo</label>
      <input type="text" class="form-control" name="" id="" aria-describedby="helpId" placeholder="">
    </div>
    <div class="form-check">
      <input class="form-check-input" type="radio" name="" id="">
      <label class="form-check-label" for="">Despachos do Processo</label>
    </div>
    <div class="form-check">
      <input class="form-check-input" type="radio" name="" id="">
      <label class="form-check-label" for="">Data de Vencimento - Despachos dos Processos</label>
    </div>
    <div class="form-check">
      <input class="form-check-input" type="radio" name="" id="">
      <label class="form-check-label" for="">Data de Vencimento do Exame</label>
    </div>
    <div class="form-check">
      <input class="form-check-input" type="radio" name="" id="">
      <label class="form-check-label" for="">Data de Vencimento das Anuidades</label>
    </div>
    <button type="button" class="btn btn-primary">Visualizar</button>
  </div>
</div>